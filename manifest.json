{
  "manifest_version": 2,
  "name": "3301-hash-alarm",
  "version": "1.0",
  "description": "Within the deep web, there exists a page that hashes to",
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "notifications",
    "tabs",
    "clipboardWrite",

    "<all_urls>"
  ],

  "background": {
    "scripts": [ "src/hashes/sha3-wrapped.js", "src/hashes/streebog-wrapped.js", "src/hashes/blake2b.js", "src/hashes/sha512.js",
		 "src/hashbox.js", "src/webrequests.js", "src/notifications.js", "background.js" ]
  }
}

